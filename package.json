{
  "name": "sce_core",
  "version": "4.0.0",
  "description": "The SCE Website",
  "scripts": {
    "api-test": "mocha",
    "frontend-test": "mocha --require @babel/register --require ignore-styles test/frontend/*.js",
    "start": "react-scripts start",
    "server": "npm start --prefix api",
    "server-install": "npm install --prefix api",
    "build": "react-scripts build",
    "lint": "./node_modules/.bin/eslint ./",
    "create-user": "npm run create-user --prefix api",
    "generate-token": "npm run generate-token --prefix api",
    "production": "rm -rf build && npm run build && npm run server",
    "start-production": "nginx -c ~/Core-v4/nginx.conf",
    "stop-production": "nginx -s stop",
    "reload-production": "nginx -s reload"
  },
  "keywords": [
    "sce_core",
    "SCE",
    "sce",
    "core",
    "v4",
    "4",
    "Software",
    "Computer",
    "Engineering"
  ],
