{
  "name": "sce_core",
  "version": "4.0.2",
  "description": "Forked from Project_MEANserver, this web application is the new SCE Core, a private Github Project. Based off of the MEAN stack, the software comes packaged with a NodeJS webserver (uses ExpressJS to handle routing/request processing), the SCE organization's home/web-services page, administrator dashboard, and members portal.",
  "main": "server.js",
  "scripts": {
    "test": "mocha",
    "start": "react-scripts build && node server.js | react-scripts start",
    "frontend": "react-scripts start",
    "deploy": "git pull && npm install && pm2 stop database api app && pm2 delete database api app && pm2 start mongod --name \"database\" && npm run build && pm2 start npm --name \"app\" -- start && pm2 start npm \"app\" -- run dev",
    "lint": "prettier 'utils/**/*.js' 'static/js/**/*.js' '__tests__/**/*.js' 'components/**/*.js' 'pages/**/*.js' 'public/**/*.js' '*.js' --write --single-quote --no-semi && standard --fix"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-semi",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "$",
      "fetch"
    ],
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjavier441/Project_MEANskills.git"
  },
  "keywords": [
    "sce_core",
    "SCE",
    "sce",
    "core",
    "v4",
    "4",
    "Software",
    "Computer",
    "Engineering"
  ],
  "author": "rjavier441",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SCE-Development/Core-v4/issues"
  },
  "homepage": "https://github.com/SCE-Development/Core-v4#readme",
  "dependencies": {
    "availity-reactstrap-validation": "^2.3.0",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.7.0",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.5",
    "ejs": "^2.7.1",
    "es6-promise": "^4.2.8",
    "express": "^4.16.2",
    "filepond": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.3",
    "node_hash": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-filepond": "^7.0.1",
    "react-iframe": "^1.8.0",
    "react-ionicons": "2.1.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.1.2",
    "reactstrap": "^8.0.1",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "standard": "^14.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8080/"
}
